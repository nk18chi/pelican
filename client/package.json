{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:4000",
  "scripts": {
    "dev": "next dev",
    "dev:test": "APP_ENV=test next dev",
    "build": "next build",
    "start": "next start",
    "build-and-start": "next build && next start",
    "lint": "eslint --cache --fix --max-warnings=0",
    "cypress": "cypress open",
    "cypress:headless": "cypress run",
    "test": "jest --maxWorkers=1",
    "test:watch": "jest --watchAll --maxWorkers=1",
    "storybook": "start-storybook -s ./public -p 6006",
    "dev-and-storybook": "next dev & start-storybook -s ./public -p 6006",
    "build-storybook": "build-storybook -s ./public",
    "download-schema": "apollo service:download --endpoint=http://localhost:4000/graphql graphql-schema.json",
    "codegen": "yarn run download-schema && apollo codegen:generate --localSchemaFile=graphql-schema.json --target=typescript --includes=gql/\\*.ts"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm test && npm run cypress:headless"
    }
  },
  "dependencies": {
    "@apollo/client": "^3.6.8",
    "@chakra-ui/icons": "^1.1.7",
    "@chakra-ui/react": "2.0.0-next.5",
    "@emotion/react": "^11",
    "@emotion/styled": "^11",
    "@stripe/react-stripe-js": "^1.16.0",
    "@stripe/stripe-js": "^1.46.0",
    "apexcharts": "^3.35.2",
    "cypress": "^8.3.1",
    "framer-motion": "^6",
    "graphql": "^15.6.0",
    "next": "^12.2.3",
    "react": "^18.2.0",
    "react-apexcharts": "^1.4.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.2",
    "react-hook-form": "^7.34.2",
    "react-icons": "^4.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@chakra-ui/storybook-addon": "2.0.0-next.4",
    "@emotion/babel-plugin": "^11.3.0",
    "@next/eslint-plugin-next": "^12.2.3",
    "@storybook/addon-actions": "^6.5.10",
    "@storybook/addon-essentials": "^6.5.10",
    "@storybook/addon-links": "^6.5.10",
    "@storybook/builder-webpack5": "^6.5.10",
    "@storybook/manager-webpack5": "^6.5.10",
    "@storybook/react": "^6.5.10",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.3.0",
    "@types/graphql": "^14.5.0",
    "@types/react": "^18.0.15",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "apollo": "^2.34.0",
    "autoprefixer": "^10.3.3",
    "babel-jest": "^27.1.0",
    "babel-loader": "^8.2.2",
    "eslint": "^8.5.0",
    "eslint-config-next": "11.1.0",
    "eslint-plugin-jest": "^26.7.0",
    "husky": "^7.0.2",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.2.4",
    "postcss": "^8.3.6",
    "react-phone-input-2": "^2.15.1",
    "react-test-renderer": "^17.0.2",
    "typescript": "^4.7.4"
  }
}
